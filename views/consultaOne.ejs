<%- include ('./partials/head.ejs') %>

    <body>
        <%- include ('./partials/navbar.ejs') %>
            <br />
            <div class="container">
                <% cadastro.forEach(cadastro=> { %>
                    <div class="card">
                        <div class="card-body">
                                <h5>Id: <%= cadastro.id %></h5>
                                <h4>Nome: <%= cadastro.nome %></h4>
                                <h5>CPF: <%= cadastro.cpf %></h5>
                                <h5>Bairro: <%= cadastro.bairro %></h5>
                                <h5>Cidade: <%= cadastro.cidade %></h5>
                                <br>
                                <a href="/deletarcadastro/<%= cadastro.id %>"><button class="btn btn-danger">Deletar</button></a>
                        </div>
                    </div>
                    <br />
                    <% }) %>
            </div>
    </body>
    <%- include ('./partials/footer.ejs') %>